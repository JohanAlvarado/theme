    / sidebar left end
    / main content start
    %section.main-content-wrapper
      %section#main-content
        / tiles start
        .row
          .col-md-3.col-sm-6
            .dashboard-tile.detail.tile-red
              .content
                %h1.text-left.timer{"data-from" => "0", "data-speed" => "2500", "data-to" => "180"}
                %p New Users
              .icon
                %i.fa.fa-users
          .col-md-3.col-sm-6
            .dashboard-tile.detail.tile-turquoise
              .content
                %h1.text-left.timer{"data-from" => "0", "data-speed" => "2500", "data-to" => "56"}
                %p New Comments
              .icon
                %i.fa.fa-comments
          .col-md-3.col-sm-6
            .dashboard-tile.detail.tile-blue
              .content
                %h1.text-left.timer{"data-from" => "0", "data-speed" => "2500", "data-to" => "32"}
                %p New Messages
              .icon
                %i.fa.fa.fa-envelope
          .col-md-3.col-sm-6
            .dashboard-tile.detail.tile-purple
              .content
                %h1.text-left.timer{"data-speed" => "2500", "data-to" => "105"}
                %p New Sales
              .icon
                %i.fa.fa-bar-chart-o
        / tiles end
        / dashboard charts and map start
        .row
          .col-md-6
            .panel.panel-default
              .panel-heading
                %h3.panel-title Sales for 2014
                .actions.pull-right
                  %i.fa.fa-chevron-down
                  %i.fa.fa-times
              .panel-body
                #sales-chart{:style => "height: 250px;"}
          .col-md-6
            .panel.panel-default
              .panel-heading
                %h3.panel-title Office locations
                .actions.pull-right
                  %i.fa.fa-chevron-down
                  %i.fa.fa-times
              .panel-body
                #map.map{:style => "height: 250px;"}
        / dashboard charts and map end
        / ToDo start
        .row
          .col-md-4
            .panel.panel-default
              .panel-heading
                %h3.panel-title To do list
                .actions.pull-right
                  %i.fa.fa-chevron-down
                  %i.fa.fa-times
              .panel-body
                .row
                  .col-md-12
                    .form-group
                      %input#new-todo.form-control{:placeholder => "What needs to be done?", :type => "text"}/
                      %section#main
                        %ul#todo-list
                    .form-group
                      %button#todo-enter.btn.btn-primary.pull-right Submit
                      #todo-count
            .row
              .col-md-12
                .panel.panel-default
                  .panel-heading
                    %h3.panel-title Server Status
                    .actions.pull-right
                      %i.fa.fa-chevron-down
                      %i.fa.fa-times
                  .panel-body
                    %span.sublabel Memory Usage
                    .progress.progress-striped
                      .progress-bar.progress-bar-info{:style => "width: 20%"} 20%
                    / progress
                    %span.sublabel CPU Usage
                    .progress.progress-striped
                      .progress-bar.progress-bar-default{:style => "width: 60%"} 60%
                    / progress
                    %span.sublabel Disk Usage
                    .progress.progress-striped
                      .progress-bar.progress-bar-primary{:style => "width: 80%"} 80%
                    / progress
          .col-md-4
            .panel.panel-solid-info
              .panel-heading
                %h3.panel-title Weather
                .actions.pull-right
                  %i.fa.fa-chevron-down
                  %i.fa.fa-times
              .panel-body
                .row
                  .col-md-6
                    %h3.text-center.small-thin.uppercase Today
                    .text-center
                      %canvas#clear-day{:height => "110", :width => "110"}
                      %h4 62°C
                  .col-md-6
                    %h3.text-center.small-thin.uppercase Tonight
                    .text-center
                      %canvas#partly-cloudy-night{:height => "110", :width => "110"}
                      %h4 44°C
              .panel-footer
                .row
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Mon
                    .text-center
                      %canvas#partly-cloudy-day{:height => "32", :width => "32"}
                      %span 48°C
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Mon
                    .text-center
                      %canvas#rain{:height => "32", :width => "32"}
                      %span 39°C
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Tue
                    .text-center
                      %canvas#sleet{:height => "32", :width => "32"}
                      %span 32°C
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Wed
                    .text-center
                      %canvas#snow{:height => "32", :width => "32"}
                      %span 28°C
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Thu
                    .text-center
                      %canvas#wind{:height => "32", :width => "32"}
                      %span 40°C
                  .col-md-2
                    %h6.text-center.small-thin.uppercase Fri
                    .text-center
                      %canvas#fog{:height => "32", :width => "32"}
                      %span 42°C
          .col-md-4
            .panel.panel-default
              .panel-body
                %h4 Browser Summary
                #donut-example
        / ToDo end
    / main content end
    / sidebar right start
