            .list-item-image
              %img.img-circle{:src => "assets/avatar4.gif"}/
            .list-item-content
              %h4 Ivan Bella
              %p Tokyo, Japan
            .item-status
          %a.list-item{:href => "javascript:void(0)"}
            .list-item-image
              %img.img-circle{:src => "assets/avatar5.gif"}/
            .list-item-content
              %h4 Gerald Carr
              %p Seattle, WA
            .item-status
          %a.list-item{:href => "javascript:void(0)"}
            .list-item-image
              %img.img-circle{:src => "assets/avatar6.gif"}/
